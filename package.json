{
  "name": "ts-vite-electron",
  "author": "aldrick",
  "description": "An app to schedule when auto shutdown your computer.",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "electron .",
    "preview": "vite preview",
    "dist:win": "vite build && electron-builder --win"
  },
  "build": {
    "appId": "com.ts-vite-electron.app",
    "files": [
      "build/**/*",
      "dist-electron/**/*",
      "node_modules/*"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "electron": "24.1.2",
    "electron-builder": "^23.6.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-electron": "^0.11.2"
  }
}
